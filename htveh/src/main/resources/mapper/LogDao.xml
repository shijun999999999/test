<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.neayon.ht.dao.LogDao">
	<insert id="saveLog">
		<![CDATA[
		insert into t_log
			(user,access,remark)
		values
			(#{user},#{access},#{remark})
		]]>
	</insert>
</mapper>